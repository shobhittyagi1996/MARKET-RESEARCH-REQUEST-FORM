<mvc:View
    controllerName="com.sap.marketresearch.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:uxap="sap.uxap"
    xmlns:core="sap.ui.core"
    xmlns:semantic="sap.f.semantic"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:upload="sap.m.upload"
    xmlns:u="sap.ui.unified"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
            <uxap:ObjectPageLayout
                id="ObjectPageLayout1"
                upperCaseAnchorBar="false"
                showAnchorBar="true"
                enableLazyLoading="true"
            >
                <uxap:sections>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection1">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection1"
                                title="GENERAL INFORMATION"
                                titleUppercase="false"
                            >
                                <uxap:blocks>
                                    <f:SimpleForm
                                        id="supplierForm1"
                                        layout="ResponsiveGridLayout"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                    >
                                        <Label
                                            id="_IDGenLabel1"
                                            text="Estimated costs of goods/works/services"
                                            required="true"
                                        />
                                        <Input
                                            id="_IDGenInput1"
                                            value=""
                                        />

                                        <Label
                                            id="_IDGenLabel2"
                                            text="Clear definition of Services/Works/Goods required"
                                            required="true"
                                        />
                                        <TextArea
                                            id="_IDGenInput2"
                                            value=""
                                        />
                                        <Label
                                            id="_IDGenLa"
                                            text="Brief Scope of Work"
                                            required="true"
                                        />
                                        <TextArea
                                            id="_IDGenI2"
                                            value=""
                                        />

                                        <Label
                                            id="_IDGenLabel3"
                                            text="Special Requirements/Criteria"
                                            required="true"
                                        />
                                        <Select
                                            id="Select"
                                            change="onSelectChangeKeyFigure"
                                            items="{aModel>/}"
                                        >
                                            <core:Item
                                                id="_IDGenItem1234"
                                                key="{aModel>text}"
                                                text="{aModel>text}"
                                            />
                                        </Select>

                                        <Label
                                            id="_IDGenLabel4"
                                            text="Type of contract"
                                            required="true"
                                        />
                                        <Select
                                            id="Sel1"
                                            change="onSelectChangeKey"
                                            items="{lModel>/}"
                                        >
                                            <core:Item
                                                id="_IDGen1234"
                                                key="{lModel>text}"
                                                text="{lModel>text}"
                                            />
                                        </Select>

                                        <Label
                                            id="_IDGenLabel5"
                                            text="HSE risk category"
                                            required="true"
                                        />
                                        <Select
                                            id="Sel2"
                                            change="onSelectChangeFigure"
                                            items="{cModel>/}"
                                        >
                                            <core:Item
                                                id="_IDGenIte2"
                                                key="{cModel>text}"
                                                text="{cModel>text}"
                                            />
                                        </Select>

                                        <Label
                                            id="_IDGenLabel6"
                                            text="Details of the existing contract (Contract number and expiration date)"
                                            required="true"
                                        />
                                        <Input
                                            id="_IDGenI1"
                                            value=""
                                        />

                                        <Label
                                            id="_IDGenL7"
                                            text="Anticipated Market research deadline"
                                            required="true"
                                        />
                                        <DatePicker
                                            id="_IDGenIn2"
                                            value=""
                                        />

                                        <Label
                                            id="_IDGenLabel8"
                                            text="Anticipated JPC meeting date"
                                            required="true"
                                        />
                                        <DatePicker
                                            id="_IDGen4"
                                            value=""
                                        />

                                        <Label
                                            id="_IDGenLabel9"
                                            text="Date of Request"
                                            required="true"
                                        />
                                        <Input
                                            id="_IDGenI0"
                                            value="09-04-2024"
                                            editable="false"
                                        />
                                    </f:SimpleForm>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>

                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection2"
                                title="Add List of Proposed Companies"
                            >
                                <uxap:blocks>
                                    <Table
                                        id="Table1"
                                        items="{myModel>/}"
                                    >
                                        <headerToolbar>
                                            <OverflowToolbar id="_IDGenTool1">
                                                <Button
                                                    id="_IDGenBun1"
                                                    icon="sap-icon://add"
                                                    type="Emphasized"
                                                    press="onCreate"
                                                />
                                                <Button
                                                    id="_IDGenButton3"
                                                    icon="sap-icon://delete"
                                                    type="Emphasized"
                                                    press="onCreate"
                                                />
                                            </OverflowToolbar>
                                        </headerToolbar>
                                        <columns>
                                            <Column id="_IDGenColn1">
                                                <Text
                                                    id="_IDGenTt2"
                                                    text="Supplier ID"
                                                />
                                            </Column>
                                            <Column id="_IDGenCol2">
                                                <Text
                                                    id="_IDGenTt3"
                                                    text="Supplier Name"
                                                />
                                            </Column>
                                            <Column id="_IDGenColn3">
                                                <Text
                                                    id="_IDGenText4"
                                                    text="Supplier Address"
                                                />
                                            </Column>
                                            <Column id="_IDGenColn4">
                                                <Text
                                                    id="_IDGenTe5"
                                                    text="Supplier deals in Commodity"
                                                />
                                            </Column>
                                            <Column id="col5">
                                                <Text
                                                    id="_IDGen10"
                                                    text="Supplier Contact"
                                                />
                                            </Column>
                                        </columns>
                                        <items>
                                            <ColumnListItem id="_IDGenColu1">
                                                <cells>
                                                    <Text
                                                        id="_IDGenT6"
                                                        text="{myModel>supplierid}"
                                                    />
                                                    <Text
                                                        id="_IDGenT7"
                                                        text="{myModel>suppliername}"
                                                    />
                                                    <Text
                                                        id="_IDGenT8"
                                                        text="{myModel>supplieraddress}"
                                                    />
                                                    <Text
                                                        id="_IDGenT9"
                                                        text="{myModel>supplierdeals}"
                                                    />
                                                    <Text
                                                        id="_IDGenT18"
                                                        text="{myModel>suppliercontact}"
                                                    />
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>

                            <uxap:ObjectPageSubSection
                                id="IDGenObjectPageSubSection1223"
                                title="List of contracts for this scope of work"
                            >
                                <uxap:blocks>
                                    <Table
                                        id="Table2"
                                        items="{Model>/}"
                                    >
                                        <headerToolbar>
                                            <OverflowToolbar id="_IDGenT2">
                                                <Button
                                                    id="_IDGenB4"
                                                    icon="sap-icon://add"
                                                    type="Emphasized"
                                                    press="onCreate"
                                                />
                                                <Button
                                                    id="_IDGen5"
                                                    icon="sap-icon://delete"
                                                    type="Emphasized"
                                                    press="onCreate"
                                                />
                                            </OverflowToolbar>
                                        </headerToolbar>
                                        <columns>
                                            <Column id="_IDColumn1">
                                                <Text
                                                    id="_IDText2"
                                                    text="Contract ID"
                                                />
                                            </Column>
                                            <Column id="_IDColumn2">
                                                <Text
                                                    id="_IDText3"
                                                    text="Contract Title"
                                                />
                                            </Column>
                                            <Column id="_IDColumn3">
                                                <Text
                                                    id="_IDText4"
                                                    text="Contract Expire date"
                                                />
                                            </Column>
                                        </columns>
                                        <items>
                                            <ColumnListItem
                                                id="_IDGenListItem2"
                                            >
                                                <cells>
                                                    <Text
                                                        id="_IDT06"
                                                        text="{Model>contractid}"
                                                    />
                                                    <Text
                                                        id="_IDTt07"
                                                        text="{Model>contracttitle}"
                                                    />
                                                    <Text
                                                        id="_IDTt08"
                                                        text="{Model>contractexpiredate}"
                                                    />
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>

                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection5"
                                title="ATTACHMENTS"
                            >
                                <uxap:blocks>
                                    <upload:UploadSetwithTable
                                        id="UploadSetTable1"
                                        sticky="ColumnHeaders"
                                        width="auto"
                                        fixedLayout="false"
                                        inset="true"
                                        autoPopinMode="true"
                                        hiddenInPopin="None"
                                        itemRenamed="onDocumentRenamedSuccess"
                                        mediaTypes="text/plain,application/msword,image/png"
                                        maxFileNameLength="60"
                                        uploadUrl="{= null }"
                                        multiple="false"
                                        items="{sModel>/}"
                                        mode="None"
                                    >
                                        <upload:columns>
                                            <Column
                                                id="filName"
                                                importance="High"
                                            >
                                                <header>
                                                    <Label
                                                        id="_IDGenLa5"
                                                        text="File Name"
                                                    />
                                                </header>
                                            </Column>
                                            <Column id="id">
                                                <header>
                                                    <Label
                                                        id="_IDGenLabel7"
                                                        text="Type"
                                                    />
                                                </header>
                                            </Column>
                                            <Column
                                                id="actionButton"
                                                importance="High"
                                            />
                                        </upload:columns>
                                        <upload:items>
                                            <upload:UploadSetwithTableItem
                                                id="_IDGenUploadSetwithTableItem1"
                                                fileName="{sModel>filename}"
                                                mediaType="{sModel>mediaType}"
                                            >
                                                <!-- Cells with cell template defining content for each column (cells and columns work with 1:1 mapping) -->
                                                <upload:cells>
                                                    <HBox id="_IDGenHBox2">
                                                        <core:Icon
                                                            id="_IDGenIcon1"
                                                            src="{sModel>mediaType}"
                                                            color="white"
                                                            class="sapMUSTItemImage sapMUSTItemIcon"
                                                        />
                                                        <VBox
                                                            id="_IDGenVBox1"
                                                            class="sapUiTinyMargin sapUiSmallMarginBegin"
                                                        >
                                                            <Link
                                                                id="_IDGenLink3"
                                                                text="{sModel>filename}"
                                                                press="openPreview"
                                                                class="sapUiTinyMarginBottom"
                                                            />
                                                            <Text
                                                                id="_IDGenText10"
                                                                class="sapUiTinyMarginTop"
                                                            />
                                                        </VBox>
                                                    </HBox>
                                                    <Text
                                                        id="_IDGenLink2"
                                                        text="{sModel>type}"
                                                    />
                                                    <Button
                                                        id="_IDGenButton2"
                                                        class="sapUiTinyMarginBegin"
                                                        type="Transparent"
                                                        icon="sap-icon://decline"
                                                        press="onRemoveHandler"
                                                    />
                                                </upload:cells>
                                            </upload:UploadSetwithTableItem>
                                        </upload:items>
                                    </upload:UploadSetwithTable>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                </uxap:sections>
            </uxap:ObjectPageLayout>
        </content>
        <footer>
            <Bar id="bar">
                <contentRight>
                    <Button text="Save as draft" type="Emphasized" />
                    <Button text="Submit for approval" type="Emphasized" />
                    <Button text="Cancel" type="Emphasized" />
                </contentRight>
            </Bar>
        </footer>
    </Page>
</mvc:View>
